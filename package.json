{
  "private": true,
  "name": "xenialauncher",
  "version": "0.0.0",
  "license": "GPL v3",
  "scripts": {
    "setup": "neu update && cp -r ./neutralino/* ./resources/",
    "move_files": "cp -r ./build/* ./resources/",
    "dev": "preact build --no-prerender && yarn move_files && neu run",
    "build": "preact build --no-prerender && yarn move_files && rm -rf ./dist/* && neu build"
  },
  "dependencies": {
    "preact": "^10.3.2",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1"
  },
  "devDependencies": {
    "preact-cli": "^3.3.5"
  }
}
